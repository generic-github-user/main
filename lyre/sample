
def function divisible
		type : [int a] [int b] -> boolean
		info : "Returns true if b evenly divides a (i.e., a % b equals 0) and false
		otherwise"
		return [not [mod a b]]


def module errors
		def class error
				def method raise

		def error access
				def error reference
				def error index
				def error import
				def error attribute
		def error not-implemented
		def error value
				def error range
				def error type
				def error argument

def function raise : ex
		print ex
		call quit

def function assert : p
		if [not p] raise [errors assertion]
def class [[hashable type T] type U]
		dictionary <: collection

		int size

		def mut method insert
		def mut method remove
		def mut method clear

		def method get
		def method contains
